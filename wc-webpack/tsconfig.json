{
  "compilerOptions": {
    "target": "es5",
    "module": "es2015",
    "moduleResolution": "node",
    "declaration": true,
    "sourceMap": true,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "outDir": "dist",
    "lib": ["es2017", "dom", "scripthost"],
    "noImplicitAny": false,
    "suppressImplicitAnyIndexErrors": true,
    "removeComments": true,
    "baseUrl": ".",
    "paths": {
      
    }
  },
  "exclude": ["node_modules", "dist"]
}
